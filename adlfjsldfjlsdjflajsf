[32m+[m[32mpackage com.example.baidumaptest;[m
[32m+[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mimport com.baidu.mapapi.SDKInitializer;[m
[32m+[m[32mimport com.baidu.mapapi.map.BaiduMap;[m
