[32m+[m[32mpublic class MainActivity extends Activity {[m
[32m+[m
[32m+[m	[32mprivate MapView mapView;[m
[32m+[m[41m	[m
[32m+[m	[32mprivate BaiduMap baiduMap;[m
[32m+[m[41m	[m
[32m+[m	[32mprivate LocationManager locationManager;[m
[32m+[m[41m	[m
[32m+[m	[32mprivate String provider;[m
[32m+[m[41m	[m
[32m+[m	[32mprivate boolean isFirstLocate = true;[m
[32m+[m[41m	[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onCreate(Bundle savedInstanceState) {[m
[32m+[m[32m        super.onCreate(savedInstanceState);[m
[32m+[m[32m        SDKInitializer.initialize(getApplicationContext());[m
[32m+[m[32m        setContentView(R.layout.activity_main);[m
[32m+[m[32m        mapView = (MapView)findViewById(R.id.map_view);[m
[32m+[m[32m        baiduMap = mapView.getMap();[m
[32m+[m[32m        baiduMap.setMyLocationEnabled(true);[m
[32m+[m[32m        locationManager = (LocationManager)getSystemService(LOCATION_SERVICE);[m
[32m+[m[32m        //ªÒ»°À˘”–ø…”√µƒŒª÷√Ã·π©∆˜[m
[32m+[m[32m        List<String> providerList = locationManager.getProviders(true);[m
[32m+[m[32m        if (providerList.contains(LocationManager.GPS_PROVIDER)){[m
[32m+[m[41m        [m	[32mprovider = LocationManager.GPS_PROVIDER;[m
[32m+[m[32m        }else if(providerList.contains(LocationManager.NETWORK_PROVIDER)){[m
[32m+[m[41m        [m	[32mprovider = LocationManager.NETWORK_PROVIDER;[m
[32m+[m[32m        }else{[m
[32m+[m[41m        [m	[32m//µ±√ª”–ø…”√µƒŒª÷√Ã·π©∆˜ ±£¨µØ≥ˆToastÃ· æ”√ªß[m
[32m+[m[41m        [m	[32mToast.makeText(this, "No location provider to use", Toast.LENGTH_SHORT).show();[m
[32m+[m[41m        [m	[32mreturn;[m
[32m+[m[32m        }[m
[32m+[m[32m        Location location = locationManager.getLastKnownLocation(provider);[m
[32m+[m[32m        if (location != null){[m
[32m+[m[41m        [m	[32mnavigateTo(location);[m
[32m+[m[32m        }[m
[32m+[m[32m        locationManager.requestLocationUpdates(provider,5000, 1, locationListener);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    private void navigateTo(Location location){[m
[32m+[m[41m    [m	[32mif (isFirstLocate){[m
[32m+[m[41m    [m		[32mLatLng ll = new LatLng(location.getLatitude(), location.getLongitude());[m
[32m+[m[41m    [m		[32mMapStatusUpdate update = MapStatusUpdateFactory.newLatLng(ll);[m
[32m+[m[41m    [m		[32mbaiduMap.animateMapStatus(update);[m
[32m+[m[41m    [m		[32mupdate = MapStatusUpdateFactory.zoomTo(16f);[m
[32m+[m[41m    [m		[32mbaiduMap.animateMapStatus(update);[m
[32m+[m[41m    [m		[32misFirstLocate = false;[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[41m    [m	[32mMyLocationData.Builder locationBuilder = new MyLocationData.Builder();[m
[32m+[m[41m    [m	[32mlocationBuilder.latitude(location.getLatitude());[m
[32m+[m[41m    [m	[32mlocationBuilder.longitude(location.getLongitude());[m
[32m+[m[41m    [m	[32mMyLocationData locationData = locationBuilder.build();[m
[32m+[m[41m    [m	[32mbaiduMap.setMyLocationData(locationData);[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    LocationListener locationListener = new LocationListener() {[m
[32m+[m[41m		[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onStatusChanged(String provider, int status, Bundle extras) {[m
[32m+[m			[32m// TODO Auto-generated method stub[m
[32m+[m[41m			[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onProviderEnabled(String provider) {[m
[32m+[m			[32m// TODO Auto-generated method stub[m
[32m+[m[41m			[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onProviderDisabled(String provider) {[m
[32m+[m			[32m// TODO Auto-generated method stub[m
[32m+[m[41m			[m
[32m+[m		[32m}[m
[32m+[m[41m		[m
[32m+[m		[32m@Override[m
[32m+[m		[32mpublic void onLocationChanged(Location location) {[m
[32m+[m			[32m// TODO Auto-generated method stub[m
[32m+[m			[32mif (location != null){[m
[32m+[m				[32mnavigateTo(location);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m[41m [m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    protected void onDestroy() {[m
[32m+[m[41m    [m	[32m// TODO Auto-generated method stub[m
[32m+[m[41m    [m	[32msuper.onDestroy();[m
[32m+[m[41m    [m	[32mbaiduMap.setMyLocationEnabled(false);[m
[32m+[m[41m    [m	[32mmapView.onDestroy();[m
[32m+[m[41m    [m	[32mif (locationManager != null){[m
[32m+[m[41m    [m		[32m//πÿ±’≥Ã–Ú ±Ω´º‡Ã˝∆˜“∆≥˝[m
[32m+[m[41m    [m		[32mlocationManager.removeUpdates(locationListener);[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
